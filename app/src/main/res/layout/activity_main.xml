<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/topenyimpanan"
        android:layout_width="336dp"
        android:layout_height="82dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/neutralbttn"
        android:elevation="20dp"
        android:text="penyimpanan"
        android:textColor="#d5d8dc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tobarangmasuk"
        android:layout_width="336dp"
        android:layout_height="82dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/neutralbttn"
        android:elevation="20dp"
        android:text="barang masuk"
        android:textColor="#d5d8dc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topenyimpanan" />

    <Button
        android:id="@+id/tobarangkeluar"
        android:layout_width="336dp"
        android:layout_height="82dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/negativebttn"
        android:elevation="20dp"
        android:text="barang keluar"
        android:textColor="#d5d8dc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tobarangmasuk"
        app:layout_constraintVertical_bias="0.025" />

</androidx.constraintlayout.widget.ConstraintLayout>